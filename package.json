{
  "name": "graphql-serverless-and-typescript",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "sls deploy",
    "deploy:prod": "sls deploy --stage prod",
    "destroy": "sls remove",
    "offline": "sls offline start --location .",
    "log": "sls logs -f postSomeData -t",
    "dynamodb-generate-seed-data": "cd src/seed-data && node -r ts-node/register  ./create_seed_data.ts",
    "dynamodb-insert-seed-data:offline": "cd src/seed-data && export IS_OFFLINE=true && node -r ts-node/register  ./insert_seed_data.ts",
    "dynamodb-insert-seed-data:prod": "cd src/seed-data && node -r ts-node/register  ./insert_seed_data.ts"
  },
  "dependencies": {
    "@okta/jwt-verifier": "0.0.15",
    "@okta/okta-sdk-nodejs": "2.0.0",
    "apollo-server-lambda": "^2.16.1",
    "aws-sdk": "^2.734.0",
    "aws-xray-sdk": "^3.1.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "node-fetch": "2.6.0",
    "source-map-support": "^0.5.10",
    "uuidv4": "^6.2.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/faker": "^4.1.12",
    "@types/jsonfile": "^6.0.0",
    "@types/node": "^14.0.27",
    "@types/serverless": "^1.72.5",
    "@types/url-search-params": "^1.1.0",
    "faker": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.5.0",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
